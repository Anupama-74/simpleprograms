int main()
{
   char str[1000], ch
   int i, frequency = 0
   print("Enter the string in which you want to find the character frequency: ");
   gets(str);
   printf("Enter a character to find the frequency in the given string: ");
   scanf("%c",&ch);
   for(i = 0; str[i] != '\0'; ++i)
   {
       if(ch == str[i])
           ++frequency;
   }
   printf("Frequency of %c = %d in the string provided", ch, frequency);
   return 0;
}
